{
  "src":[
    "./build/index.js",
    "./src/*.js",
    "./src/**/*.js",
    "./README.md"
  ],
  "dest":"./docs",
  "clean": true,
  "jsdoc":{
    "hierarchy":true,
    "sort":"kind",
    "recurse": true,
    "plugins":[
      "plugins/markdown"
    ]
  },
  "markdown":{
    "sanitize":false
  },
  "app":{
    "title":"RGAPI4JS | A Typescript wrapper for Riot Games' apis.",
    "routing":"query",
    "entrance":"content:readme",
    "base":"/rgapi4js",
    "server": "github"
  },
  "template":{
    "options":{
      "title":{
        "label":"rgapi4js",
        "href":"."
      },
      "sidebar":{
        "enabled":true,
        "outline":"tree",
        "collapsed":false,
        "toolbar":true,
        "itemsFolded":false,
        "itemsOverflow":"crop",
        "badges":true,
        "search":true,
        "animations":true
      },
      "symbols":{
        "autoLink":true,
        "params":"list",
        "enums":"list",
        "props":"list",
        "meta":false
      },
      "navbar":{
        "enabled":true,
        "dark":true,
        "animations":true,
        "menu":[
          {
            "label":"Readme",
            "href":"."
          },
          {
            "label":"Documentation",
            "href":"?api"
          },
          {
            "label":"GitHub",
            "href":"https://github.com/Hi-Ray/rgapi4js"
          }
        ]
      }
    }
  }
}
