{
  "name": "rgapi4js",
  "version": "0.0.1",
  "main": "build/lib/index.js",
  "license": "GPL-3.0",
  "scripts": {
    "test": "mocha --config .mocharc.json lib/test/**/*.test.ts",
    "test:watch": "mocha --config .mocharc.json --watch lib/test/**/*.test.ts",
    "typescript:compile": "tsc -p .",
    "lint": "eslint lib/**/*.ts",
    "lint:fix": "eslint lib/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,md}\"",
    "doc:generate": "yarn typescript:compile && docma -c docma.json --clean",
    "doc:serve": "docma serve",
    "readmetoc": "doctoc README.md --github"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "dependencies": {
    "@fightmegg/riot-rate-limiter": "^0.0.12",
    "@types/chai": "^4.2.18",
    "axios": "^1.6.0",
    "axios-rate-limit": "^1.3.0",
    "lodash": "^4.17.21",
    "redis": "^3.1.2"
  },
  "devDependencies": {
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "axios-mock-adapter": "^1.19.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "docma": "^3.2.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "mocha-logger": "^1.0.7",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "redis-mock": "^0.56.3",
    "ts-interface-checker": "^1.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
